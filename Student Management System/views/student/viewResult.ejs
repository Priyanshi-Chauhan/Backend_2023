<header>
  <div class="container-fluid bg-info text-center" style="height: 50px">

    <div class="row">
      <div class="col-sm-10">
        <h4 style="padding-top: 8px">Student Management system</h4>
      </div>
      <div class="col-sm-2">
        <a href="/"><button style="margin-top: 3px" type="button" class="btn btn-primary">Logout</button></a>
      </div>
    </div>
    <div>
    </div>
  </div>
</header>
<br>
<div class="container">
  <h4>Student Result</h4>
  <br>
  <div class="stuTable">
    <table class="table table-striped">
      <thead>
        <tr>
          <th scope="col">Roll no.</th>
          <th scope="col">Name</th>
          <th scope="col">Date of birth</th>
          <th scope="col">Score</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>
            <%= one.r %>
          </td>
          <td>
            <%= one.name%>
          </td>
          <td>
            <%= one.dob %>
          </td>
          <td>
            <%= one.score %>
          </td>
        </tr>
      </tbody>
    </table>
  </div>
</div>